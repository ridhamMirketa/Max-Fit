<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Add_Error</fullName>
    <active>true</active>
    <description>If add error checkbox is checked that means there is record duplicacy. Then give error on speaker field.</description>
    <errorConditionFormula>Add_Error__c = true</errorConditionFormula>
    <errorDisplayField>Speaker__c</errorDisplayField>
    <errorMessage>Duplicate speaker  found</errorMessage>
</ValidationRule>
